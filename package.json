{
  "name": "hissin-bastid",
  "version": "0.1.0",
  "description": "NodeJS Typescript BattleSnake",
  "repository": {
    "type": "git",
    "url": "git+https:http://github.com/bkmaibach/hissin-bastid"
  },
  "author": "Keith Maibach",
  "license": "MIT",
  "scripts": {
    "postinstall": "mkdir -p logs/game",
    "start": "ts-node src/index.ts",
    "start4": "PORT=9001 npm run serve & PORT=9002 npm run serve & PORT=9003 npm run serve & PORT=9004 npm run serve &",
    "stop4": "fuser -k 9001/tcp & fuser -k 9002/tcp & fuser -k 9003/tcp & fuser -k 9004/tcp",
    "build-engine-board": "cd $GOPATH/src/github.com/battlesnakeio/engine && make run & cd $CODEPATH/board && npm i",
    "setup-board": "npm run start4 && $GOPATH/bin/engine server & cd $CODEPATH/board && npm start &",
    "takedown-board": "npm run stop4 & fuser -k 3000/tcp & fuser -k 3004/tcp && fuser -k 3005/tcp",
    "1game": "scripts/new_game.sh",
    "start-run": "npm run start4 && npm run 1game",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@types/app-root-path": "^1.2.4",
    "app-root-path": "^2.1.0",
    "async": "^2.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "bson": "^4.0.1",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "l1-path-finder": "^1.0.0",
    "lodash": "^4.17.5",
    "ndarray": "^1.0.18",
    "winston": "^2.4.4",
    "ts-node": "^5.0.1",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.35",
    "@types/connect-mongo": "^0.0.35",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.120",
    "@types/lusca": "^1.5.0",
    "@types/mongodb": "^3.1.15",
    "@types/mongoose": "^4.7.40",
    "@types/morgan": "^1.7.35",
    "@types/ndarray": "^1.0.6",
    "@types/node": "^9.6.40",
    "@types/nodemailer": "^4.6.5",
    "@types/passport": "^0.4.7",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.6",
    "@types/winston": "^2.4.4",
    "concurrently": "^3.6.1",
    "jest": "^22.4.4",
    "nodemon": "^1.18.7",
    "shelljs": "^0.8.3",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter/issues"
  },
  "homepage": "https://github.com/Microsoft/TypeScript-Node-Starter#readme",
  "main": "jest.config.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "12.16.1"
  }
}
